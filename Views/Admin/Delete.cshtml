@model MVC_Final.Models.Specialization

@* @{
    ViewData["Title"] = "Delete";
} *@

<h1>Delete Specialization</h1>

<div>
    <hr />
    <h1>Name: @Model.Name</h1>

    @* <h1>Doctors: @Model.Doctors</h1> *@
    <div>
        @foreach (var doctor in Model.Doctors)
        {
            <div>Dr:  @doctor.Name </div>
        }
    </div>
    <img width="200" height="200" href="~/image/@Model.Cover" />
</div>
<div>
    <a asp-action="index" asp-controller="Admin" class="btn btn-info">Back to List</a>
     <a asp-action="ConfirmDelete" asp-controller="Admin" asp-route-id="@Model.Id" class="btn btn-danger">Delete</a>
</div>
